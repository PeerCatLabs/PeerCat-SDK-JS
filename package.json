{
  "name": "@peercat/sdk",
  "version": "0.1.0",
  "description": "Official TypeScript/JavaScript SDK for the PeerCat AI image generation API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "peercat",
    "ai",
    "image-generation",
    "stable-diffusion",
    "imagen",
    "solana",
    "api",
    "sdk"
  ],
  "author": "PeerCat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peercat/peercat-sdk-js"
  },
  "homepage": "https://peerc.at",
  "bugs": {
    "url": "https://github.com/peercat/peercat-sdk-js/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "dependencies": {}
}
